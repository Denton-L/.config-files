[user]
	email = liu.denton@gmail.com
	name = Denton Liu
[push]
	default = simple
[alias]
	git = !git
	no-ff = merge --no-ff
	graph = log --graph
	please = push --force-with-lease
	ignore = update-index --assume-unchanged
	unignore = update-index --no-assume-unchanged
	amend = commit --amend --no-edit
	prune-merged = !git-prune-merged
	delete = !git-delete
	push-up = !git-push-up
	squash = !git-squash
[diff]
	tool = vimdiff
[difftool]
	prompt = false
	trustExitCode = true
	guitool = gvimdiff
[merge]
	conflictStyle = diff3
	tool = vimdiff
[mergetool]
	prompt = false
	trustExitCode = true
	guitool = gvimdiff
[core]
	excludesfile = ~/.globalgitignore
[help]
	autocorrect = 10
[rerere]
	enabled = true
[rebase]
	autosquash = true
[init]
	templatedir = ~/.git-template
